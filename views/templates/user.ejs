<li class="list-group-item"> <%= user.username %> 
     <% if (user.user_type == "admin"){ %>
        <input class="btn btn-primary" type="button" value="demote" onclick="window.location.href='/admin/demote?user=<%= user.username %>'"
     <% } else { %>
        <input class="btn btn-primary" type="button" value="promote" onclick="window.location.href='/admin/promote?user=<%= user.username %>'">
     <% } %>
     
</li>